<p id="notice"><%= notice %></p>

<h1>Reverse Look Ups</h1>

<table>
  <thead>
    <tr>
      <th>Title</th>
      <th>Keywords</th>
      <th>Searchablekeys</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @reverse_look_ups.each do |reverse_look_up| %>
      <tr>
        <td><%= reverse_look_up.title %></td>
        <td><%= reverse_look_up.keyWords %></td>
        <td><%= reverse_look_up.searchableKeys %></td>
        <td><%= link_to 'Show', reverse_look_up %></td>
        <td><%= link_to 'Edit', edit_reverse_look_up_path(reverse_look_up) %></td>
        <td><%= link_to 'Destroy', reverse_look_up, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Reverse Look Up', new_reverse_look_up_path %>
